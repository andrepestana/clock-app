{
  "name": "quasar_typescript",
  "version": "0.0.3",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "author": "Andre",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
    "test:unit": "jest --updateSnapshot --verbose",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "predeploy": "quasar build",
    "deploy": "gh-pages -d dist/spa"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vue/eslint-config-standard": "^6.0.0",
    "axios": "^0.18.1",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "core-js": "^3.6.5",
    "flush-promises": "^1.0.2",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "quasar": "^1.0.0",
    "ts-jest": "^26.5.4",
    "vue-class-component": "^7.2.2",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "^8.3.0",
    "vuex-class": "^0.3.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-qcalendar": "^3.4.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.1",
    "@types/jest": "^26.0.21",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "gh-pages": "^3.2.3",
    "typescript": "^4.3.5"
  },
